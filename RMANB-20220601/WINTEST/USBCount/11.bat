@echo off
:B
IF EXIST USB.LOG DEL USB.LOG
CALL USBVIEW.EXE
ECHO PRESS P TO USBPORT TEST PASS.........
ECHO PRESS F TO USBPORT TEST FAIL.........

ECHO P=PASS
ECHO F=FAIL

CHOICE /C:PF

IF ERRORLEVEL 2 goto FAIL
IF ERRORLEVEL 1 goto PASS


:FAIL
ECHO FAIL > USB.LOG
GOTO END

:PASS
ECHO PASS > USB.LOG
GOTO END


:end


